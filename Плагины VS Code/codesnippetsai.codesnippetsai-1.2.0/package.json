{
	"name": "codesnippetsai",
	"displayName": "Code Snippets AI - GPT-4",
	"description": "Turn Your Questions Into Code with GPT-4. Works with JavaScript, TypeScript, Python, C++, Java, PHP, Ruby & Others.",
	"version": "1.2.0",
	"publisher": "codesnippetsai",
	"icon": "resources/extensionIcon.png",
	"license": "GPL",
	"repository": {
		"url": "https://github.com/stefanfmeyer/codesnippets.ai-extension"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Machine Learning",
		"Data Science",
		"Linters",
		"Testing",
		"Education",
		"Debuggers",
		"Extension Packs",
		"Other"
	],
	"keywords": [
		"codesnippets",
		"codesnippets.ai",
		"openai",
		"gpt3",
		"gpt3.5",
		"gpt4",
		"chatgpt",
		"bard",
		"lamda",
		"keybindings",
		"codex",
		"codegpt",
		"cohere",
		"copilot",
		"codesnippetsAi",
		"ai",
		"copilot",
		"generate",
		"debug",
		"debugging",
		"find bugs",
		"explain",
		"refactor",
		"snippets",
		"assistant",
		"generate",
		"code debug",
		"code debugging",
		"code explain",
		"code refactor",
		"code snippets",
		"code assistant",
		"nodejs",
		"snippets",
		"node",
		"react",
		"ruby",
		"python",
		"php",
		"typescript",
		"csharp",
		"kotlin",
		"css",
		"c",
		"bash",
		"rust",
		"objective-c",
		"html",
		"copilot"
	],
	"activationEvents": [
		"onView:codesnippetsai.chatView",
		"onCommand:codesnippetsai.ask",
		"onCommand:codesnippetsai.explain",
		"onCommand:codesnippetsai.refactor",
		"onCommand:codesnippetsai.optimize",
		"onCommand:codesnippetsai.findProblems",
		"onCommand:codesnippetsai.documentation",
		"onCommand:codesnippetsai.saveSnippet"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codesnippetsai.ask",
				"title": "Code Snippets AI"
			},
			{
				"command": "codesnippetsai.ask",
				"title": "Ask AI"
			},
			{
				"command": "codesnippetsai.explain",
				"title": "AI -> Explain"
			},
			{
				"command": "codesnippetsai.refactor",
				"title": "AI -> Refactor"
			},
			{
				"command": "codesnippetsai.debug",
				"title": "AI -> Debug"
			},
			{
				"command": "codesnippetsai.documentation",
				"title": "AI -> Documentation"
			},
			{
				"command": "codesnippetsai.resetConversation",
				"title": "Reset AI conversation"
			},
			{
				"command": "codesnippetsai.tryAgain",
				"title": "Retry AI request"
			},
			{
				"command": "codesnippetsai.saveSnippet",
				"title": "Save Code Snippet"
			}
		],
		"keybindings": [
			{
				"command": "codesnippetsai.ask",
				"key": "ctrl+shift+a",
				"mac": "cmd+shift+a",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "codesnippetsai.ask",
					"when": "editorTextFocus",
					"group": "codesnippetsai-menu-group@1"
				},
				{
					"command": "codesnippetsai.explain",
					"when": "editorHasSelection",
					"group": "codesnippetsai-menu-group@2"
				},
				{
					"command": "codesnippetsai.refactor",
					"when": "editorHasSelection",
					"group": "codesnippetsai-menu-group@3"
				},
				{
					"command": "codesnippetsai.debug",
					"when": "editorHasSelection",
					"group": "codesnippetsai-menu-group@4"
				},
				{
					"command": "codesnippetsai.documentation",
					"when": "editorHasSelection",
					"group": "codesnippetsai-menu-group@6"
				},
				{
					"command": "codesnippetsai.saveSnippet",
					"when": "editorHasSelection",
					"title": "Save Code Snippet",
					"group": "codesnippetsai-menu-group@7"
				}
			],
			"commandPalette": [
				{
					"command": "codesnippetsai.ask"
				},
				{
					"command": "codesnippetsai.explain",
					"when": "editorHasSelection"
				},
				{
					"command": "codesnippetsai.refactor",
					"when": "editorHasSelection"
				},
				{
					"command": "codesnippetsai.debug",
					"when": "editorHasSelection"
				},
				{
					"command": "codesnippetsai.documentation",
					"when": "editorHasSelection"
				},
				{
					"command": "codesnippetsai.resetConversation"
				},
				{
					"command": "codesnippetsai.saveSnippet",
					"when": "editorHasSelection"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "codesnippetsai",
					"title": "Code Snippets AI",
					"icon": "resources/icon.png"
				}
			]
		},
		"views": {
			"codesnippetsai": [
				{
					"type": "webview",
					"id": "codesnippetsai.chatView",
					"name": "AI"
				},
				{
					"id": "codesnippetsai.treeView",
					"name": "Snippets Library",
					"icon": "database",
					"viewType": "firestoreTreeView",
					"canToggleVisibility": true,
					"treeView": {
						"treeDataProvider": "firestoreTreeDataProvider"
					}
				}
			]
		},
		"configuration": {
			"title": "codesnippetsai",
			"type": "object",
			"properties": {
				"codesnippetsai.accessToken": {
					"markdownDescription": "Access Token from your CodeSnippets.ai Account",
					"type": "string",
					"order": 0
				},
				"codesnippetsai.pasteOnClick": {
					"type": "boolean",
					"default": true,
					"description": "Paste the code from a codeblock inside the response into the editor when you click on it",
					"order": 2
				},
				"codesnippetsai.promptPrefix.explain": {
					"type": "string",
					"default": "Explain what this code does:  ",
					"description": "The prompt prefix used for explaining the selected code",
					"order": 3
				},
				"codesnippetsai.promptPrefix.refactor": {
					"type": "string",
					"default": "Refactor this code and explain what's changed: ",
					"description": "The prompt prefix used for refactoring the selected code",
					"order": 4
				},
				"codesnippetsai.promptPrefix.debug": {
					"type": "string",
					"default": "Debug with the following code, fix the issues and explain what was wrong (Do not change anything else, if there are no problems say so): ",
					"description": "The prompt prefix used for debugging the selected code",
					"order": 5
				},
				"codesnippetsai.promptPrefix.documentation": {
					"type": "string",
					"default": "Write documentation for the following code:  ",
					"description": "The prompt prefix used for writing documentation for the selected code",
					"order": 7
				},
				"codesnippetsai.timeoutLength": {
					"type": "number",
					"default": "120",
					"description": "How long should the request wait for a response before timing out (in seconds)",
					"order": 9
				},
				"codesnippetsai.selectedInsideCodeblock": {
					"type": "boolean",
					"default": true,
					"description": "Append selected code as a codeblock (```...code...```) instead of plain text",
					"order": 10
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"update-package-lock": "npm install --package-lock-only"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"ts-loader": "^9.4.1",
		"typescript": "^4.9.3",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.0"
	},
	"dependencies": {
		"@firebase/firestore": "^3.8.3",
		"@google-cloud/firestore": "^6.4.3",
		"axios": "^1.3.4",
		"chatgpt": "^5.0.4",
		"firebase": "^9.17.1",
		"firebase-admin": "^11.5.0",
		"gpt3-tokenizer": "^1.1.5",
		"node-fetch": "^3.3.0",
		"zod": "^3.20.6"
	},
	"__metadata": {
		"id": "18a6fd50-025e-4b92-9fc2-2b46deb812e6",
